#, sh-format
msgid "option:"
msgstr "オプション:"

#: lib/functions.sh:223
#, sh-format
msgid "file not found."
msgstr "ファイルが見つかりません。"

#: lib/functions.sh:233 lib/provision.sh:34
#, sh-format
msgid "please input valid email address."
msgstr "有効なEメールアドレスを指定してください。"

#: lib/functions.sh:243
#, sh-format
msgid "please input on/off."
msgstr "onかoffを指定してください。"

#: lib/functions.sh:263
#, sh-format
msgid "Done."
msgstr "完了しました。"

#: lib/functions.sh:266
#, sh-format
msgid "Failed."
msgstr "失敗しました。"

#: lib/provision.sh:13
#, sh-format
msgid "please input 'en_US' or 'ja'."
msgstr "'en_US' か 'ja' を指定してください。"

#: lib/provision.sh:24 lib/provision.sh:58
#, sh-format
msgid "please input valid hostname."
msgstr "有効なホスト名を指定してください。"

#: lib/provision.sh:43
#, sh-format
msgid "please input valid port number."
msgstr "有効なポート番号を指定してください。"

#: lib/provision.sh:68
#, sh-format
msgid "please input [a-zA-Z0-9._-] 3 to 64 characters."
msgstr "[a-zA-Z0-9._-] の組み合わせの、3～64文字の文字列を指定してください。"

#: lib/provision.sh:78
#, sh-format
msgid "Enter username for database. USE [a-zA-Z0-9._-] 3 to 16 characters."
msgstr "データベースユーザ名を指定してください。3～16文字の[a-zA-Z0-9._-]を含む文字列が使用できます。"

#: lib/provision.sh:99
#, sh-format
msgid "Enter admin username for WordPress. USE [a-zA-Z0-9._-] 4 characters minimum."
msgstr "WordPressの管理者ユーザを指定してください。4文字以上の[a-zA-Z0-9._-] の文字列が使用できます。"

#: lib/provision.sh:112 lib/provision.sh:119
#, sh-format
msgid "can not be specified with another db system."
msgstr "他のDBと共に指定することは出来ません。"

#: lib/provision.sh:123
#, sh-format
msgid "can not be specified."
msgstr "指定することが出来ません。"

#: lib/provision.sh:144
#, sh-format
msgid "Enter Title for for WordPress. USE 1 characters minimum."
msgstr "WordPressのタイトルを指定してください。1文字以上の文字列が使用できます。"

#: lib/provision.sh:237
#, sh-format
msgid "not implimented."
msgstr "実装されていません。"

#: lib/provision.sh:241 lib/provision.sh:247 lib/provision.sh:253
#: lib/provision.sh:259 lib/provision.sh:265
#, sh-format
msgid "can not specified with another application."
msgstr "他のアプリケーションと共に指定することは出来ません。"

#: lib/provision.sh:400 lib/ssl.sh:96
#, sh-format
msgid "Cannot use option"
msgstr "このオプションは使用できません。"

#: lib/provision.sh:418
#, sh-format
msgid "option --nginx and --httpd is can not specify both at the same time."
msgstr "オプション--nginxと--httpdを同時に指定することは出来ません。"

#: lib/provision.sh:425
#, sh-format
msgid "Target name requires [a-zA-Z0-9._-] 3-24 characters."
msgstr "ターゲット名は3～24文字の[a-zA-Z0-9._-]の文字列を指定してください。"

#: lib/provision.sh:436
#, sh-format
msgid "require option --fqdn for your website. ex) kusanagi.tokyo."
msgstr "Webサイトで使用するホスト名(FQDN)を入力してください。 例) kusanagi.tokyo"

#: lib/provision.sh:550
#, sh-format
msgid "Cannot get Let's Encrypt SSL Certificate files."
msgstr "Let's EncryptのSSL証明書を取得できませんでした。"

#: lib/ssl.sh:109
#, sh-format
msgid "specified in same time."
msgstr ""

#: lib/ssl.sh:112
#, sh-format
msgid "can not specified in same time."
msgstr "同時に指定することは出来ません。"
