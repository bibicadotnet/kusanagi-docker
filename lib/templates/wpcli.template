## CONFIG
  config:
    container_name: ${PROFILE}_config
    restart: always
${WSL1_BUILD}
${WSL1_CONTEXTS}
    user: "1000:1001"
    env_file:
      - .kusanagi
      - .kusanagi.wp
      - .kusanagi.db
    volumes:
      - database:/var/run/mysqld
      - kusanagi:/home/kusanagi
      ${WSL_INI}
    command: wp --version

